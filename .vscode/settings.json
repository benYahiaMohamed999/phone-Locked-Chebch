{
    "dart.flutterSdkPath": null,
    "dart.sdkPath": null,
    "dart.debugExternalPackageLibraries": false,
    "dart.debugSdkLibraries": false,
    "dart.showInspectorNotificationsForWidgetErrors": true,
    "dart.previewFlutterUiGuides": true,
    "dart.previewFlutterUiGuidesCustomTracking": true,
    "dart.closingLabels": true,
    "dart.lineLength": 80,
    "dart.enableSdkFormatter": true,
    "dart.organizeImportsOnSave": true,
    "dart.fixAllOnSave": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.organizeImports": "explicit"
    },
    "files.associations": {
        "*.dart": "dart"
    },
    "emmet.includeLanguages": {
        "dart": "html"
    },
    "search.exclude": {
        "**/build/": true,
        "**/.dart_tool/": true,
        "**/.flutter-plugins": true,
        "**/.flutter-plugins-dependencies": true,
        "**/.packages": true,
        "**/pubspec.lock": true
    },
    "files.exclude": {
        "**/.dart_tool": true,
        "**/.flutter-plugins": true,
        "**/.flutter-plugins-dependencies": true,
        "**/.packages": true,
        "**/build/": true
    },
    "dart.devToolsLocation": "external",
    "dart.devToolsTheme": "dark",
    "dart.flutterHotReloadOnSave": "all",
    "dart.flutterScreenshotPath": "screenshots",
    "dart.flutterSdkPaths": [],
    "dart.flutterVersion": "3.0.0",
    "dart.analysisExcludedFolders": [
        "**/build/**",
        "**/.dart_tool/**"
    ]
}